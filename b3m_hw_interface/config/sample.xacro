<?xml version="1.0"?>
<robot name="sample">
  <link name="base_link"/>
  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" lower="-100" upper="100" velocity="1000"/>
  </joint>
  <link name="link1"/>
  <joint name="joint2" type="revolute">
    <parent link="base_link"/>
    <child link="link2"/>
    <origin xyz="0 0.5 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="100" lower="-100" upper="100" velocity="1000"/>
  </joint>
  <link name="link2"/>
</robot>
